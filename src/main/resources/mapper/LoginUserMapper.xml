<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.chatmindai.springboot3learn.mapper.LoginUserMapper">
    <select id="login" parameterType="net.chatmindai.springboot3learn.entity.loginUser" resultType="net.chatmindai.springboot3learn.entity.loginUser">
        SELECT * FROM loginuser WHERE username = #{username} AND password = #{password}
    </select>
</mapper>