<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chatmindai.springboot3learn.mapper.LoginUserMapper">

    <select id="login" parameterType="com.chatmindai.springboot3learn.entity.loginUser" resultType="com.chatmindai.springboot3learn.entity.loginUser">
        SELECT * FROM loginuser WHERE username = #{username} AND password = #{password}
    </select>

</mapper>
